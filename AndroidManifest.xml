<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brainjuice"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.brainjuice.BrainJuice"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.brainjuice.QuestionPage"
            android:label="@string/app_name" >
        </activity>       
        <activity
            android:name="com.example.brainjuice.Questionpage"
            android:label="@string/title_activity_questionpage"
            android:parentActivityName="com.example.brainjuice.BrainJuice" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.brainjuice.BrainJuice" />
        </activity>
        <activity
            android:name="com.example.brainjuice.Faq"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.brainjuice.AskQuestionPopUp"
            android:label="@string/app_name" >
        </activity>
        
    </application>

</manifest>
